{
  "name": "epoll",
  "version": "5.0.0-0",
  "description": "A low-level Node.js binding for the Linux epoll API",
  "main": "epoll.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "lint": "jshint *.js example/*/*.js test/*.js test/*/*.js",
    "test": "cd test && ./run-tests && cd ..",
    "install": "pkg-prebuilds-verify ./binding-options.js || node-gyp rebuild"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fivdi/epoll.git"
  },
  "engines": {
    "node": ">=18.0"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "pkg-prebuilds": "^0.2.1"
  },
  "devDependencies": {
    "jshint": "^2.13.6"
  },
  "keywords": [
    "epoll",
    "epollpri",
    "poll",
    "pollpri",
    "gpio",
    "interrupt",
    "linux"
  ],
  "author": "fivdi",
  "license": "MIT",
  "gypfile": true
}
